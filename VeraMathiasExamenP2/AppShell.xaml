<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="VeraMathiasExamenP2.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:VeraMathiasExamenP2"
    Title="VeraMathiasExamenP2">

    <FlyoutItem x:Name="PrincipalFLY" Icon="icon_home.png">
        <ShellContent
            Title="Ingreso de datos"
            ContentTemplate="{DataTemplate local:VeraMathiasPrincipal}"
            Route="MainPage" /> 
    </FlyoutItem>

    <FlyoutItem x:Name="ApellidosFLY" Icon="icon_home.png">
        <ShellContent
        Title="Apellidos y nombres"
        ContentTemplate="{DataTemplate local:VeraMathiasApellidos}"
        Route="MainPage" />
    </FlyoutItem>

</Shell>
