<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="VeraMathiasExamenP2.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:VeraMathiasExamenP2"
    Title="VeraMathiasExamenP2">

    <TabBar>
        <Tab Title="Principal" Icon="home.jpg" x:Name="mveraPrincipalTB">
            <ShellContent
                Title="Ingreso de Datos"
                ContentTemplate="{DataTemplate local:VeraMathiasPrincipal}"
                Route="VeraMathiasPrincipal" />
        </Tab>
        <Tab Title="Apellidos" Icon="person.jpg" x:Name="mveraApellidosTB">
            <ShellContent
                Title="Nombres y Apellidos"
                ContentTemplate="{DataTemplate local:VeraMathiasApellidos}"
                Route="VeraMathiasApellidos" />
        </Tab>
    </TabBar>

</Shell>
